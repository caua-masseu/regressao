Package: regressao
Title: Ajustar modelo de regressão linear
Version: 0.0.0.9000
Authors@R: 
    person("Cauã", "Masseu", , "c204238@dac.unicamp.br", role = c("aut", "cre")),
    person("Marcos", "Moribe", , "m236225@dac.unicamp.br", role = c("aut", "cre")),
    person("Rodrigo", "Gomes", , "r186777@dac.unicamp.br", role = c("aut", "cre")),
